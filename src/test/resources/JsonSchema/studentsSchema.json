            "state": {
              "type": "string"
            },
            "city": {
              "type": "string"
            }
          },
          "required": [
            "pincode",
            "state",
            "city"
          ]
        },
        "currentAddress": {
          "type": "object",
          "properties": {
            "pincode": {
              "type": "number"
            },
            "state": {
              "type": "string"
            },
            "city": {
              "type": "string"
            }
          },
          "required": [
            "pincode",
            "state",
            "city"
          ]
        }
      },
      "required": [
        "country",
        "permanentAddress",
        "currentAddress"
      ]
    },
    "gender": {
      "type": "boolean"
    }
  },
  "required": [
    "id",
    "fullName",
    "education",
    "dob",
    "studentaddress",
    "gender"
  ]
}