      "properties": {
        "graduation": {
          "type": "string"
        },
        "postGraduation": {
          "type": "string"
        }
      },
      "required": [
        "graduation",
        "postGraduation"
      ]
    },
    "dob": {
      "type": "object",
      "properties": {
        "date": {
          "type": "number"
        },
        "month": {
          "type": "string"
        },
        "year": {
          "type": "number"
        }
      },
      "required": [
        "date",
        "month",
        "year"
      ]
